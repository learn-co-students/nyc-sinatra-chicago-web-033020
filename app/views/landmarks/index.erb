<h2>All Historical Landmarks</h2>

<% @landmarks.each do |landmark| %>
  <a href="landmarks/<%= landmark.id%>">
    <h3><%= landmark.name %></h3>
    <p>Year completed: <%= landmark.year_completed%></p>
    <br>
  </a>
<% end %>

